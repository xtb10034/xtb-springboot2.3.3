        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xtb.restful.mapper.AccountMapper">
    <select id="getAccount" resultType="com.xtb.restful.bean.Account">
        select * from account where id = #{id}
